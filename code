algoritmo "Fibonacci"
var
  C, N1, N2 : Inteiro
Procedimento NextFibonacci (var A, B: Inteiro)
var C: Inteiro
Inicio
  C := A+B
  Escreva (C)
  A := B
  B := C
FimProcedimento
Inicio
  Escreva "Esta é a sucessão de Leonardo Pisa vulgo Fibonacci: "
  N1 <- 0
  EscrevaL (N1)
  N2 <- 1
  Escreva (N2)
  Para C := 3 ate 10 faca NextFibonacci(N1, N2)
  FimPara
Fimalgoritmo
